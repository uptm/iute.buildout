<?xml version="1.0"?>

<object>
    <order manager="plone.portaltop" skinname="iute.thememain">
        <viewlet name="iute.themebase.toplevelnav" insert-before="*" />
    	<viewlet name="iute.themebase.banner" insert-after="iute.themebase.toplevelnav" />
        <viewlet name="iute.currentsectionnav" insert-after="iute.themebase.banner" />
        <viewlet name="iute.themebase.mainimage" insert-after="iute.currentsectionnav" />
		<viewlet name="iute.simpleemergency" insert-before="iute.currentsectionnav" />
    </order>
    <order manager="plone.portalfooter" skinname="iute.thememain">
        <viewlet name="iute.themebase.footer" insert-before="plone.footer" />
        <viewlet name="iute.themebase.colophon" insert-before="plone.colophon" />
    </order>
    <order manager="plone.abovecontent" skinname="iute.thememain">
        <viewlet name="iute.themebase.path_bar" insert-before="*" />
    </order>
    <order manager="plone.belowcontent" skinname="iute.thememain">
        <viewlet name="iute.themebase.documentbyline" insert-after="*" />
    </order>
		
    <hidden manage="plone.portaltop" skinname="iute.thememain">
		<viewlet name="iute.simpleemergency" />
		<viewlet name="plone.personal_bar" />
		<viewlet name="plone.path_bar" />
	</hidden>
	<hidden manager="plone.belowcontenttitle" skinname="iute.thememain">
		<viewlet name="plone.belowcontenttitle.documentbyline" />
	</hidden>
    <hidden manager="plone.portalfooter" skinname="iute.thememain">
	    <viewlet name="plone.footer" />
		<viewlet name="plone.colophon" />
    </hidden>
    <hidden manager="plone.portalheader" skinname="iute.thememain">
	    <viewlet name="plone.searchbox" />
		<viewlet name="plone.logo" />
		<viewlet name="plone.global_sections" />
		<viewlet name="plone.site_actions" />
    </hidden>
</object>
