<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="iute.thememain">

 <object name="iute_thememain_custom_images"
    meta_type="Filesystem Directory View"
    directory="iute.thememain:skins/iute_thememain_custom_images"/>
 <object name="iute_thememain_custom_templates"
    meta_type="Filesystem Directory View"
    directory="iute.thememain:skins/iute_thememain_custom_templates"/>
 <object name="iute_thememain_styles"
    meta_type="Filesystem Directory View"
    directory="iute.thememain:skins/iute_thememain_styles"/>

 <skin-path name="iute.thememain" based-on="Plone Default">
  	<layer name="iute_thememain_custom_images" 		insert-after="custom"/>
  	<layer name="iute_thememain_custom_templates" 		insert-after="iute_thememain_custom_images"/>
  	<layer name="iute_thememain_styles"				insert-after="iute_thememain_custom_templates"/>
	<layer name="iute_theme_styles" 					insert-after="iute_thememain_styles"/>
	<layer name="iute_theme_custom_images" 			insert-after="iute_theme_styles"/>
	<layer name="iute_theme_custom_templates" 			insert-after="iute_theme_custom_images"/>
 </skin-path>

</object>
