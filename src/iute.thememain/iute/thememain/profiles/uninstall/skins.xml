<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Plone Default">

   <object name="iute_thememain_custom_images"
       meta_type="Filesystem Directory View"
       directory="iute.thememain:skins/iute_thememain_custom_images"
       remove="True"
    />
    <object name="iute_thememain_custom_templates"
        meta_type="Filesystem Directory View"
        directory="iute.thememain:skins/iute_thememain_custom_templates"
        remove="True"
    />
    <object name="iute_thememain_styles"
        meta_type="Filesystem Directory View"
        directory="iute.thememain:skins/iute_thememain_styles"
        remove="True"
    />

    <skin-path name="iute.thememain" based-on="Plone Default" remove="True">
        <layer name="iute_thememain_custom_images"
            insert-after="custom"
            remove="Treu"
        />
        <layer name="iute_thememain_custom_templates"
            insert-after="iute_thememain_custom_images"
            remove="True"
        />
        <layer name="iute_thememain_styles"
            insert-after="iute_thememain_custom_templates"
            remove="True"
        />
 </skin-path>

</object>
