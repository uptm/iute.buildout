<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="iute.themecollege">

 <object name="iute_themecollege_custom_images"
    meta_type="Filesystem Directory View"
    directory="iute.themecollege:skins/iute_themecollege_custom_images"/>
 <object name="iute_themecollege_custom_templates"
    meta_type="Filesystem Directory View"
    directory="iute.themecollege:skins/iute_themecollege_custom_templates"/>
 <object name="iute_themecollege_styles"
    meta_type="Filesystem Directory View"
    directory="iute.themecollege:skins/iute_themecollege_styles"/>

 <skin-path name="iute.themecollege" based-on="Plone Default">
	<layer name="iute_themecollege_custom_images" 		insert-after="custom"/>
	<layer name="iute_themecollege_custom_templates"	insert-after="iute_themecollege_custom_images"/>
	<layer name="iute_themecollege_styles"			insert-after="iute_themecollege_custom_templates"/>
	<layer name="iute_theme_styles" 			insert-after="iute_themecollege_styles"/>
	<layer name="iute_theme_custom_images" 			insert-after="iute_theme_styles"/>
	<layer name="iute_theme_custom_templates" 		insert-after="iute_theme_custom_images"/>
 </skin-path>

</object>
