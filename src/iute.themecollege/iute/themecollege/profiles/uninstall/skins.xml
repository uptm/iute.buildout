<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Plone Default">

   <object name="iute_themecollege_custom_images"
       meta_type="Filesystem Directory View"
       directory="iute.themecollege:skins/iute_themecollege_custom_images"
       remove="True"
    />
    <object name="iute_themecollege_custom_templates"
        meta_type="Filesystem Directory View"
        directory="iute.themecollege:skins/iute_themecollege_custom_templates"
        remove="True"
    />
    <object name="iute_themecollege_styles"
        meta_type="Filesystem Directory View"
        directory="iute.themecollege:skins/iute_themecollege_styles"
        remove="True"
    />

    <skin-path name="iute.themecollege" remove="True" based-on="Plone Default">
        <layer name="iute_themecollege_custom_images"
            insert-after="custom"
            remove="True"
        />
        <layer name="iute_themecollege_custom_templates"
            insert-after="iute_themecollege_custom_images"
            remove="True"
        />
        <layer name="iute_themecollege_styles"
            insert-after="iute_themecollege_custom_templates"
            remove="True"/>
 </skin-path>

</object>
